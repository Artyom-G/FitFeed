{
  "cli": {
    "version": ">= 7.8.5",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_ANDROID_CLIENT_ID": "$GOOGLE_ANDROID_CLIENT_ID",
        "GOOGLE_IOS_CLIENT_ID": "$GOOGLE_IOS_CLIENT_ID",
        "GOOGLE_WEB_CLIENT_ID": "$GOOGLE_WEB_CLIENT_ID"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GOOGLE_ANDROID_CLIENT_ID": "$GOOGLE_ANDROID_CLIENT_ID",
        "GOOGLE_IOS_CLIENT_ID": "$GOOGLE_IOS_CLIENT_ID",
        "GOOGLE_WEB_CLIENT_ID": "$GOOGLE_WEB_CLIENT_ID"
      }
    },
    "production": {
      "env": {
        "GOOGLE_ANDROID_CLIENT_ID": "$GOOGLE_ANDROID_CLIENT_ID",
        "GOOGLE_IOS_CLIENT_ID": "$GOOGLE_IOS_CLIENT_ID",
        "GOOGLE_WEB_CLIENT_ID": "$GOOGLE_WEB_CLIENT_ID"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
